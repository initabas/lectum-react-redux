{
    "name": "redux-intensive-app",
    "version": "0.1.0",
    "author": "Dima Vakatsiienko <d.vakatsiienko@lectrum.io>",
    "private": true,
    "scripts": {
        "start-js": "node scripts/start.js",
        "start": "npm run start-js",
        "test": "node scripts/test.js --env=jsdom",
        "setup": "sh scripts/setup.sh",
        "build": "node scripts/build.js",
        "check": "run-s lint:eslint lint:scss-lint",
        "lint:eslint": "eslint ./src",
        "lint:scss-lint": "scss-lint"
    },
    "dependencies": {
        "classnames": "^2.2.5",
        "gsap": "^1.20.2",
        "history": "^4.7.2",
        "immutable": "^3.8.2",
        "moment": "^2.18.1",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "react-redux": "^5.0.6",
        "react-redux-form": "^1.16.0",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^5.0.0-alpha.8",
        "react-transition-group": "^2.2.0",
        "redux": "^3.7.2",
        "redux-saga": "^1.0.0-beta.0",
        "redux-thunk": "^2.2.0",
        "socket.io-client": "~2.1.0",
        "uuid": "^3.1.0"
    },
    "devDependencies": {
        "@babel/core": "~7.0.0-beta.42",
        "@babel/plugin-proposal-class-properties": "~7.0.0-beta.42",
        "@babel/plugin-proposal-decorators": "~7.0.0-beta.44",
        "@babel/plugin-proposal-function-bind": "~7.0.0-beta.42",
        "@babel/plugin-proposal-object-rest-spread": "~7.0.0-beta.42",
        "@babel/polyfill": "~7.0.0-beta.42",
        "@babel/preset-env": "~7.0.0-beta.42",
        "@babel/preset-react": "~7.0.0-beta.42",
        "autoprefixer": "8.2.0",
        "babel-core": "^7.0.0-0",
        "babel-eslint": "8.2.2",
        "babel-jest": "~23.0.0-alpha.4",
        "babel-loader": "~8.0.0-beta.2",
        "case-sensitive-paths-webpack-plugin": "2.1.2",
        "chalk": "2.3.2",
        "css-loader": "0.28.11",
        "dotenv": "5.0.1",
        "enzyme": "^3.1.0",
        "enzyme-adapter-react-16": "^1.0.1",
        "eslint": "4.19.1",
        "eslint-config-react-app": "~3.0.0-next.b2fd8db8",
        "eslint-loader": "2.0.0",
        "eslint-plugin-flowtype": "2.46.1",
        "eslint-plugin-import": "2.9.0",
        "eslint-plugin-jest": "^21.3.2",
        "eslint-plugin-jsx-a11y": "6.0.3",
        "eslint-plugin-react": "~7.7.0",
        "extract-text-webpack-plugin": "3.0.2",
        "file-loader": "0.11.2",
        "fs-extra": "3.0.1",
        "html-webpack-plugin": "2.29.0",
        "jest": "^22.4.3",
        "node-sass": "^4.5.3",
        "npm-run-all": "^4.0.2",
        "object-assign": "4.1.1",
        "postcss-flexbugs-fixes": "3.3.0",
        "postcss-loader": "2.1.3",
        "prettier": "1.11.1",
        "prettier-eslint": "8.8.1",
        "prettier-eslint-cli": "4.7.1",
        "promise": "8.0.1",
        "raf": "^3.4.0",
        "react-dev-utils": "^3.1.0",
        "react-hot-loader": "~4.0.0",
        "react-test-renderer": "^16.0.0",
        "redux-logger": "^3.0.6",
        "redux-mock-store": "^1.3.0",
        "sass-loader": "^6.0.6",
        "style-loader": "0.18.2",
        "sw-precache-webpack-plugin": "0.11.5",
        "url-loader": "1.0.1",
        "webpack": "3.5.1",
        "webpack-dev-server": "2.7.1",
        "webpack-manifest-plugin": "1.2.1",
        "whatwg-fetch": "2.0.4"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}"
        ],
        "setupFiles": [
            "<rootDir>/config/polyfills.js",
            "raf/polyfill",
            "<rootDir>/config/mocks.js"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.js?(x)",
            "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "modulePaths": [
            "<rootDir>/src"
        ],
        "moduleFileExtensions": [
            "web.js",
            "js",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    }
}
